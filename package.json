{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "landing page for goodcall.nyc",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --pushstate --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html} dist && cp -r assets dist'",
    "stage-push": "surge dist good-call-nyc-staging.surge.sh",
    "stage": "export NODE_ENV=staging; npm run build && npm run dist && npm run stage-push",
    "deploy-push": "s3-website deploy dist",
    "deploy": "export NODE_ENV=production; npm run build && npm run dist && npm run deploy-push"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/good-call-nyc/landing-page"
  },
  "author": "data-doge",
  "license": "",
  "bugs": {
    "url": "https://github.com/good-call-nyc/landing-page/issues"
  },
  "homepage": "https://github.com/good-call-nyc/landing-page",
  "dependencies": {
    "material-ui": "^0.15.4",
    "node-uuid": "^1.4.7",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-inline-grid": "^0.5.3",
    "react-responsive": "^1.1.4",
    "react-router": "^2.7.0",
    "react-router-scroll": "^0.3.2",
    "react-scroll": "^1.3.1",
    "react-slick": "^0.14.1",
    "react-tap-event-plugin": "^1.0.0",
    "render-if": "^0.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "browserify-css": "^0.9.1",
    "budo": "^4.0.0",
    "s3-website": "^2.1.0",
    "surge": "^0.18.0",
    "uglifyify": "^3.0.1"
  }
}
