{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "landing page for goodcall.nyc",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --pushstate --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "dist": "mkdir -p dist && bash -c 'cp index.html dist && cp bundle.js dist && cp -r assets dist && cp sitemap.xml dist'",
    "stage-push": "surge dist good-call-nyc-staging.surge.sh",
    "stage-copy-html": "cp -r dist/index.html dist/200.html",
    "stage": "export NODE_ENV=production; export IS_STAGING=true; npm run build && npm run dist && npm run stage-copy-html && npm run stage-push",
    "deploy-push": "s3-website deploy dist",
    "deploy": "export NODE_ENV=production; npm run build && npm run dist && npm run deploy-push"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/good-call-nyc/landing-page"
  },
  "author": "data-doge",
  "license": "",
  "bugs": {
    "url": "https://github.com/good-call-nyc/landing-page/issues"
  },
  "homepage": "https://github.com/good-call-nyc/landing-page",
  "dependencies": {
    "bowser": "1.4.5",
    "camelize": "1.0.0",
    "isomorphic-fetch": "2.2.1",
    "lodash.assign": "4.2.0",
    "lodash.includes": "4.3.0",
    "lodash.isempty": "4.4.0",
    "lodash.isplainobject": "4.0.6",
    "lodash.range": "3.2.0",
    "material-ui": "0.15.4",
    "moment": "2.15.1",
    "node-uuid": "1.4.7",
    "object.map": "0.2.0",
    "react": "15.3.1",
    "react-cookie": "0.4.8",
    "react-dom": "15.3.1",
    "react-inline-grid": "0.5.3",
    "react-responsive": "1.1.4",
    "react-router": "2.7.0",
    "react-router-scroll": "0.3.2",
    "react-scroll": "1.3.1",
    "react-slick-data-doge-fork": "0.14.5",
    "react-tap-event-plugin": "1.0.0",
    "render-if": "0.1.1",
    "rotate-array": "1.1.0",
    "snakeize": "0.1.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babelify": "7.3.0",
    "browserify": "10.0.0",
    "browserify-css": "0.9.1",
    "budo": "4.0.0",
    "envify": "3.4.1",
    "s3-website": "2.1.0",
    "surge": "0.18.0",
    "uglifyify": "3.0.1"
  }
}
