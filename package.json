{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "landing page for goodcall.nyc",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html} dist'",
    "push": "s3-website deploy dist",
    "deploy": "export NODE_ENV=production; npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/good-call-nyc/landing-page"
  },
  "author": "data-doge",
  "license": "",
  "bugs": {
    "url": "https://github.com/good-call-nyc/landing-page/issues"
  },
  "homepage": "https://github.com/good-call-nyc/landing-page",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "browserify-css": "^0.9.1",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "uglifyify": "^3.0.1"
  }
}
