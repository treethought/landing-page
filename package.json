    "lodash.foreach": "^4.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.pick": "^4.4.0",
    "lodash.some": "^4.6.0",
    "lodash.values": "^4.3.0",
    "material-ui": "^0.18.3",
    "moment": "^2.15.1",
    "node-uuid": "^1.4.7",
    "object.map": "^0.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-update": "^15.4.2",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.6.1",
    "react-grecaptcha": "^1.1.4",
    "react-inline-grid": "^0.5.3",
    "react-responsive": "^1.1.4",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.2",
    "react-scroll": "^1.3.1",
    "react-slick": "^0.14.11",
